{
    "name": "themonkeys/laravel-google-auth",
    "description": "Laravel Authentication Driver for Google OAuth authentication",
    "authors": [
        {
            "name": "The Monkeys",
            "email": "developers@themonkeys.com.au"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "4.0.x",
        "google/google-api-php-client": "0.6.x"
    },
    "autoload": {
        "psr-0": {
            "Themonkeys\\LaravelGoogleAuth": "src/"
        }
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "google/google-api-php-client",
                "version": "0.6.7",
                "dist": {
                    "url": "http://google-api-php-client.googlecode.com/files/google-api-php-client-0.6.7.tar.gz",
                    "type": "tar"
                },
                "autoload": {
                    "classmap": ["src/"]
                }
            }
        }
    ],
    "minimum-stability": "dev"
}